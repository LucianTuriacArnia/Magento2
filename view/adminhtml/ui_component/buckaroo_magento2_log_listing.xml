<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">buckaroo_magento2_log_listing.buckaroo_magento2_log_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>buckaroo_magento2_log_columns</spinner>
		<deps>
			<dep>buckaroo_magento2_log_listing.buckaroo_magento2_log_listing_data_source</dep>
		</deps>
	</settings>
	<dataSource name="buckaroo_magento2_log_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">log_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Buckaroo_Magento2::Log</aclResource>
		<dataProvider name="buckaroo_magento2_log_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>log_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="buckaroo_magento2_log_columns">
		<selectionsColumn name="ids">
			<settings>
				<indexField>log_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="log_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="log_id">
			<settings>
				<filter>text</filter>
				<label translate="true">log_id</label>
			</settings>
		</column>
		<column name="channel">
			<settings>
				<filter>text</filter>
				<label translate="true">channel</label>
			</settings>
		</column>
		<column name="level">
			<settings>
				<filter>text</filter>
				<label translate="true">level</label>
			</settings>
		</column>
		<column name="message">
			<settings>
				<filter>text</filter>
				<label translate="true">message</label>
			</settings>
		</column>
		<column name="timestamp">
			<settings>
				<filter>text</filter>
				<label translate="true">timestamp</label>
			</settings>
		</column>
		<column name="session_id">
			<settings>
				<filter>text</filter>
				<label translate="true">session_id</label>
			</settings>
		</column>
		<column name="customer_id">
			<settings>
				<filter>text</filter>
				<label translate="true">customer_id</label>
			</settings>
		</column>
		<column name="quote_id">
			<settings>
				<filter>text</filter>
				<label translate="true">quote_id</label>
			</settings>
		</column>
		<column name="order_id">
			<settings>
				<filter>text</filter>
				<label translate="true">order_id</label>
			</settings>
		</column>
	</columns>
</listing>
